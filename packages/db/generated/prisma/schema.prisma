generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:mysecretpassword@localhost:5432/postgres"
}

model User {
  id       String  @id @default(uuid())
  name     String
  email    String  @unique
  password String
  OTP      String?
}
